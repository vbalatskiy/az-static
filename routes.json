{
    "routes": [
      {
        "route": "/*",
        "serve": "/index_map.html",
        "allowedRoles": ["reader"]
      } 
    ],
    "platformErrorOverrides": [
      {
        "errorType": "Unauthorized_Unknown",
        "serve": "/.auth/login/aad"
      },
              {
        "errorType": "Unauthenticated",
        "serve": "/.auth/login/aad"
      },
              {
        "errorType": "Unauthorized_InsufficientUserInformation",
        "serve": "/.auth/login/aad"
      },
              {
        "errorType": "Unauthorized_MissingRoles",
        "serve": "/.auth/login/aad"
      }
    ]
  }
